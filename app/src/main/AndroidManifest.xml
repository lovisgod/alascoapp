<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ayo.dsapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/aim_account"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="DSAPP"
            android:theme="@style/myTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".productActivity"
            android:label="OUR PRODUCTS"
            android:theme="@style/myTheme" />
        <activity
            android:name=".landingActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".accountActivity"
            android:label="ACCOUNT LINKING"
            android:parentActivityName=".landingActivity" />
        <activity android:name=".webPageActivity" />
        <activity
            android:name=".testimonialsActivity"
            android:label="TESTIMONY"
            android:parentActivityName=".landingActivity"
            android:theme="@style/myTheme" />
        <activity android:name=".addCommentActivity"></activity>
    </application>

</manifest>